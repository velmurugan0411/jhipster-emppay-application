
application {
  config {
    baseName lmsApp,
    applicationType monolith,
    packageName com.techfinite.lms,
    authenticationType jwt,
    prodDatabaseType postgresql,
    clientFramework angular
  }
  entities *
}

entity Courses {
	courseId Integer
    courseTitle String
	courseDesc String
}

entity Schedules {
	scheduleId Integer
	scheduleName String
    scheduleBeginDate Date
    scheduleEndDate Date
}
    